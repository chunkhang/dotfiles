account personal
host smtp.gmail.com
port 465
protocol smtp
auth on
user chunkhang@gmail.com
from chunkhang@gmail.com
tls on
tls_starttls off
passwordeval "security find-internet-password -s smtp.gmail.com -a chunkhang@gmail.com -w"

